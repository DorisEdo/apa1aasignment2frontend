{
    "name": "supabase-frontend-template",
    "version": "1.0.0",
    "description": "A secure message fetcher for Supabase",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "client": "cd client && npm run dev",
      "client:build": "cd client && npm run build",
      "dev:both": "concurrently \"npm run dev\" \"npm run client\""
    },
    "dependencies": {
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "node-fetch": "^3.3.2"
    },
    "devDependencies": {
      "concurrently": "^8.2.0",
      "nodemon": "^3.0.1"
    },
    "type": "module"
  }